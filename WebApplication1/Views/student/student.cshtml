@model List<WebApplication1.Models.student>
@{
    ViewBag.Title = "student";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int sn = 0;
}

<h2>student</h2>
<style>
    table, td, th {
        background-color: blue;
    }
    </style>

<a href="@Url.Action("Create")">Add New Record</a>

<h2>employee list</h2>
<table class="table" border="1">
    <tr>
        <th>id</th>
        <th> name</th>
        <th> phone</th>

    </tr>
    @foreach (var data in Model)
    {
        <tr>
            <td>@(++sn)</td>
            <td>@data.name</td>
            <td>@data.phone</td>
          
                                       
        </tr>



    }
</table>






